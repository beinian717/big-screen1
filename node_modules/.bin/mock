#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/songzhikun/Documents/front/myProject/big-screen/big-screen-react/node_modules/.pnpm/echart@0.1.3/node_modules/echart/bin/node_modules:/Users/songzhikun/Documents/front/myProject/big-screen/big-screen-react/node_modules/.pnpm/echart@0.1.3/node_modules/echart/node_modules:/Users/songzhikun/Documents/front/myProject/big-screen/big-screen-react/node_modules/.pnpm/echart@0.1.3/node_modules:/Users/songzhikun/Documents/front/myProject/big-screen/big-screen-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/songzhikun/Documents/front/myProject/big-screen/big-screen-react/node_modules/.pnpm/echart@0.1.3/node_modules/echart/bin/node_modules:/Users/songzhikun/Documents/front/myProject/big-screen/big-screen-react/node_modules/.pnpm/echart@0.1.3/node_modules/echart/node_modules:/Users/songzhikun/Documents/front/myProject/big-screen/big-screen-react/node_modules/.pnpm/echart@0.1.3/node_modules:/Users/songzhikun/Documents/front/myProject/big-screen/big-screen-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../echart/bin/mock" "$@"
else
  exec node  "$basedir/../echart/bin/mock" "$@"
fi
